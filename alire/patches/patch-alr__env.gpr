--- alr_env.gpr.orig	2023-10-19 15:04:21.000000000 +0000
+++ alr_env.gpr
@@ -36,6 +36,7 @@ aggregate project Alr_Env is
 
    case Alire_Common.Host_Os is
       when "freebsd" => for External ("GNATCOLL_OS") use "unix";
