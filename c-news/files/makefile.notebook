MACROS=	-ms
ROFF=	groff
NROFF=	nroff


all :	ps more

ps :	locking.ps makefiles.ps

locking.ps :	locking
	${ROFF} ${MACROS} ${.ALLSRC} >${.TARGET}

makefiles.ps:	makefiles
	${ROFF} ${MACROS} ${.ALLSRC} >${.TARGET}


more :	locking.more makefiles.more

locking.more :	locking
	${NROFF} ${MACROS} ${.ALLSRC} >${.TARGET}

makefiles.more:	makefiles
	${NROFF} ${MACROS} ${.ALLSRC} >${.TARGET}
