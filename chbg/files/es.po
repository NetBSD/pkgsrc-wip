msgid "Use -h option to print full list of available options\n"
msgstr ""
"Use la opción -h para ver la lista completa de opciones disponibles\n"

#: config.c:461
#, c-format
msgid ""
"Usage:  %s  [options] [any number of pictures]\n"
"%s-%s\n"
msgstr ""
"Uso:  %s  [opciones] [imágenes]\n"
"%s-%s\n"

#: config.c:464
msgid "\t-v                    - print version number and exit\n"
msgstr "\t-v                    - muestra el número de versión y sale\n"

#: config.c:465
msgid "\t-h                    - print this help message and exit\n"
msgstr "\t-h                    - muestra este mensaje de ayuda y sale\n"

#: config.c:466
msgid "\t-setup                - start setup window\n"
msgstr "\t-setup                - abre la ventana de configuración\n"

#: config.c:467
msgid "\t-scenario $file       - fetch parameters from file $file\n"
msgstr "\t-scenario $file       - carga los parámetros desde el archivo $file\n"

#: config.c:468
msgid ""
"\t-mode $type           - set picture rendering mode, mode is\n"
"\t                        one of following:\n"
"\t                        : tile\n"
"\t                        : mirror\n"
"\t                        : center\n"
"\t                        : maximize\n"
"\t                        : smart\n"
"\t                        : centertile\n"
"\t                        : inttile\n"
"\t                        : symtile\n"
"\t                        : intmirror\n"
"\t                        : symmirror\n"
msgstr ""
"\t-mode $type           - escoge el modo de la imagen, el modo es\n"
"\t                        uno de los siguientes :\n"
"\t                        : mosaico\n"
"\t                        : espejo\n"
"\t                        : centrado\n"
"\t                        : maximizar\n"
"\t                        : personal\n"
"\t                        : mosaico-centrado\n"
"\t                        : mosaico-entero\n"
"\t                        : mosaico-simetrico\n"
"\t                        : espejo-entero\n"
"\t                        : espejo-simétrico\n"
"\n"

#: config.c:480
#, c-format
msgid "\t-effect $nr           - effect type for changing pictures [0-%d]\n"
msgstr "\t-effect $nr            - tipo de efecto para el cambio de imágenes [0-%d]\n"

#: config.c:481
msgid "\t-bg $color            - set default background color 1\n"
msgstr "\t-bg $color            - escoge el color de fondo 1 por defecto\n"

#: config.c:482
msgid "\t-bg2 $color           - set default background color 2\n"
msgstr "\t-bg2 $color           - escoge el color de fondo 2 por defecto\n"

#: config.c:483
msgid "\t-rand_colors          - randomize background colors\n"
msgstr "\t-rand_colors          - colores de fondo aleatorios\n"

#: config.c:484
#, c-format
msgid ""
"\t-shader $nr           - set background color shading effect\n"
"\t                        type [0-%d]\n"
msgstr ""
"\t-shader $nr   - escoge el tipo de efecto de sombreado del color\n"
"\t                        de fondo [0-%d]\n"

#: config.c:486
msgid ""
"\t-interval $start[-$end]\n"
"\t                      - time between picture changes\n"
msgstr ""
"\t-interval $start[-$end]\n"
"\t                      - tiempo entre cambios de imagen\n"

#: config.c:488
msgid ""
"\t-max_grow $nr         - maximal growing ratio of picture in\n"
"\t                        smart mode\n"
msgstr ""
"\t-max_grow $nr         - proporción máxima creciente de imagen \n"
"\t                        modo personal\n"

#: config.c:490
msgid ""
"\t-max_size $nr         - maximal size of picture in %% of screen size\n"
"\t                        used in smart mode, acceptable value 10-100\n"
msgstr ""
"\t-max_size $nr         - tamaño máximo de imagen en %% del tamaño de pantalla\n"
"\t                        en modo personal, valores aceptables 10-100\n"

#: config.c:492
msgid "\t-randomize            - randomize picture order\n"
msgstr "\t-randomize            - orden de imágenes al azar\n"

#: config.c:493
msgid "\t-screensaver          - act as screensaver\n"
msgstr "\t-screensaver          - usar como protector de pantalla\n"

#: config.c:495
msgid "\t-xscreensaver         - act as xscreensaver client\n"
msgstr "\t-xscreensaver         - usar como cliente xscreensaver\n"

#: config.c:497
msgid ""
"\t-speed $nr            - speed ratio for picture changing effects\n"
"\t                        (lower is faster)\n"
msgstr ""
"\t-speed $nr            - velocidad para los efectos de cambio de imágenes\n"
"\t                        (cifra inferior es más rápido)\n"

#: config.c:499
msgid "\t-blank                - show blank screen, when can't load picture\n"
msgstr ""
"\t-blank                - mostrar pantalla en blanco si la imagen no puede\n"
"\t                        cargar\n"

#: config.c:500
msgid ""
"\t-cycle_blank          - when pictures not present cycle with blanking\n"
"\t                        screen (optionally with shading)\n"
msgstr ""
"\t-cycle_blank          - cuando no hay imagen, presentar pantalla en\n"
"\t                        blanco (opcionalmente con sombreado)\n"

#: config.c:502
msgid "\t-R                    - recurse through directory\n"
msgstr "\t-R                    - incluir los subdirectorios\n"

#: config.c:503
msgid ""
"\t-pattern $ptrn        - wildcard pattern for picture selection in\n"
"\t                        recursion through directories\n"
msgstr ""
"\t-pattern $ptrn        - filtrar patrón para selección de imagen\n"
"\t                        cuando se incluyen subdirectorios\n"

#: config.c:505
msgid "\t-inwindow             - run in own window not on root\n"
msgstr "\t-inwindow             - ejecutar en ventana propia, no en root\n"

#: config.c:506
msgid ""
"\t-windowid $id         - run in existing X window with known window ID\n"
msgstr ""
"\t-windowid $id         - ejecutar en ventana X existente con ID conocida "
"connu\n"

#: config.c:507
msgid ""
"\t-run                  - start runining, after setup window is popped up\n"
"\t                        (have effect only when used with -setup option)\n"
msgstr ""
"\t-run                  - iniciar después que la ventana de configuración\n"
"\t                        es iniciada (sólo funciona con la opción -setup)\n"

#: config.c:509
msgid ""
"\t-min_psize $nr        - minimal allowed size of picture when recursing\n"
"\t                        directories\n"
msgstr ""
"\t-min_psize $nr        - tamaño mínimo permitido de imagen cuando se\n"
"\t                        incluyen subdirectorios\n"

#: config.c:511
msgid "\t-deffects $list       - list of disabled effects\n"
msgstr "\t-deffects $list       - listado de efectos desactivados\n"

#: config.c:512
msgid "\t-dshaders $list       - list of disabled shaders\n"
msgstr "\t-dshaders $list       - listado de matices desactivados \n"

#: config.c:513
msgid "\t-notooltips           - disable tooltips in GUI setup\n"
msgstr ""
"\t-notooltips           - desactivar tooltips en la interfaz "
"gráfica\n"

#: config.c:514
msgid ""
"\t-friter $nr           - number of iterations for fractal based shaders\n"
msgstr ""
"\t-friter $nr           - número de iteraciones para matices basados en "
"fractales\n"

#: config.c:515
msgid ""
"\t-rect_size $nr        - size of rectangle used in picture changing\n"
"\t                        effects\n"
msgstr ""
"\t-min_psize $nr        - tamaño de rectángulo usado en efectos de\n"
"\t                        cambio de imágenes\n"

#: config.c:517
msgid "\t-grad_dir $dir        - supply path to GIMP gradients directory\n"
msgstr ""
"\t-grad_dir $dir        - suministrar ruta al directorio de "
"degradados del GIMP\n"

#: config.c:518
msgid "\t-use_grad             - use GIMP gradients for shading background\n"
msgstr ""
"\t-use_grad             - usar gradientes del GIMP para sombreado de "
"fondos\n"

#: config.c:519
msgid "\t-grad $nr             - number of selected gradient (0 = random)\n"
msgstr "\t-grad $nr             - número de sombreado elegido (0 = al azar)\n"

#: config.c:520
msgid "\t-remember             - remember last picture\n"
msgstr "\t-remember             - recordar la última imagen\n"

#: config.c:521
msgid "\t-sort                 - sort picture alphabeticaly\n"
msgstr "\t-sort                 - clasificar imágenes alfabéticamente\n"

#: config.c:523
msgid ""
"\t-enlightenment        - set desktop background via Enlightenment WM\n"
"\t                        when available\n"
msgstr ""
"\t-enlightenment        - escoger el fondo de pantalla via Enlightenment\n"
"\t                        cuando se disponga\n"

#: config.c:526
msgid ""
"\t-once                 - run one cycle of background changing and exit\n"
msgstr ""
"\t-once                 - ejecutar un ciclo de cambio de fondos y salir\n"

#: config.c:527
msgid ""
"\t-thumb $type          - show thumbnail view dialog, choose one of\n"
"\t                        all - all thumbnails in one window\n"
"\t                        dir - make thumbnail pages by directories\n"
"\t                        page - show 25 thumbnails per window\n"
msgstr ""
"\t-thumb $type          - mostrar diálogo de miniaturas, escoger "
"uno de :\n"
"\t                        todos - todas las miniaturas en una ventana\n"
"\t                        dir - una página de miniaturas por directorio\n"
"\t                        página - mostrar 25 miniaturas por ventana\n"

#: config.c:531
msgid ""
"\t-xpos $pos            - horizontal picture position on screen\n"
"\t                        in smart mode (0.0-1.0)\n"
msgstr ""
"\t-xpos $pos            - posición horizontal de la imagen en pantalla\n"
"\t                        en modo personal (0.0-1.0)\n"

#: config.c:533
msgid ""
"\t-ypos $pos            - vertical picture position on screen\n"
"\t                        in smart mode (0.0-1.0)\n"
msgstr ""
"\t-ypos $pos            - posición vertical de la imagen en pantalla\n"
"\t                        en modo personal (0.0-1.0)\n"

#: config.c:535
msgid "\t-use_tiles            - use picture tiles for picture background\n"
msgstr "\t-use_tiles            - usar mosaico de imágenes para fondo\n"

#: config.c:536
msgid ""
"\t-add_tile $file       - directory or file which will be used for tiles\n"
msgstr ""
"\t-add_tile $file       - directorio o archivo que será usado para mosaicos "
"mosaïques\n"

#: config.c:537
msgid "\t-tile $file           - default tile for picture background\n"
msgstr "\t-tile $file           - mosaico por defecto para fondo\n"

#: config.c:538
msgid "\t-use_banners          - put banner on top of background picture\n"
msgstr ""
"\t-use_banners          - poner banner por sobre de la imagen de fondo\n"

#: config.c:539
msgid ""
"\t-add_banner $file     - directory or file which will be used for banners\n"
msgstr ""
"\t-add_banner $file     - directorio o archivo que será usado para "
"banners\n"

#: config.c:540
msgid "\t-banner $file         - use image as banner\n"
msgstr "\t-banner $file         - usar imagen como banner\n"

#: config.c:541
msgid "\t-banner_xpos $pos     - horizontal banner position on screen\n"
msgstr ""
"\t-banner_xpos $pos     - posición horizontal del banner en pantalla\n"

#: config.c:542
msgid "\t-banner_ypos $pos     - vertical banner position on screen\n"
msgstr ""
"\t-banner_ypos $pos     - posición vertical del banner en pantalla\n"

#: config.c:543
msgid "\t-banner_max_grow $nr  - maximal growing ratio of banner\n"
msgstr ""
"\t-banner_max_grow $nr  - proporción máxima para agrandar banner\n"

#: config.c:544
msgid "\t-banner_max_size $nr  - maximal size of banner in %% of screen size\n"
msgstr ""
"\t-banner_max_size $nr  - tamaño máximo del banner en %% del tamaño "
"de pantalla\n"

#: config.c:545
#, c-format
msgid "\t-banner_mode $mode    - banner painting mode (0-%d)\n"
msgstr "\t-banner_mode $mode    - modo de dibujo de banner (0-%d)\n"

#: config.c:546
msgid ""
"\t-to_file              - generate picture into file instead of to window\n"
msgstr ""
"\t-to_file            - generar imagen en archivo en lugar de "
"una ventana\n"

#: config.c:547
msgid "\t-out_file $file       - name of file in which will be image written\n"
msgstr ""
"\t-out_file $file       - nombre de archivo de la nueva imagen\n"

#: config.c:548
msgid ""
"\t-out_file_width $nr   - width of picture when writing to file\n"
"\t                        0 means - take width of root window\n"
msgstr ""
"\t-out_file_width $nr   - ancho de la imagen cuando se escribe en "
