--- libsanitizer/configure.tgt.orig	2014-02-08 12:31:10.000000000 +0000
+++ libsanitizer/configure.tgt
@@ -32,7 +32,7 @@ case "${target}" in
 	;;
   arm*-*-linux*)
 	;;
-  x86_64-*-darwin[1]* | i?86-*-darwin[1]*)
+  x86_64-*-darwin[1][1-9]* | i?86-*-darwin[1][1-9]*)
 	TSAN_SUPPORTED=no
 	;;
   *)
