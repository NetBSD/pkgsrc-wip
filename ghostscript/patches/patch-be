 #define return_error(err) return gs_note_error(err)
 
 #if !defined(__STDC_VERSION__) || __STDC_VERSION__ < 199901L
+# if (defined(__func__))
+#  undef __func__
+# endif
 #  if defined(__GNUC__) && __GNUC__ >= 2
 #    define __func__ __FUNCTION__
